{
  "expo": {
    "name": "dozie",
    "slug": "dozie",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.dozie"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.dozie"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "react-native-bottom-tabs",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "expo-dynamic-app-icon",
        {
          "default": {
            "image": "./assets/images/icon.png",
            "prerendered": true
          },
          "dark": {
            "image": "./assets/images/icon-dark.png",
            "prerendered": true
          },
          "green": {
            "image": "./assets/images/icon-green.png",
            "prerendered": true
          },
          "blue": {
            "image": "./assets/images/icon-blue.png",
            "prerendered": true
          },
          "orange": {
            "image": "./assets/images/icon-orange.png",
            "prerendered": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
